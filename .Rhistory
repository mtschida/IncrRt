IncerData <- function(file1 = "http://www.bjs.gov/nps/resources/documents/QT_imp%20rate_tot.xlsx",sheetIndex1=1, startRow1=10, endRow1 = 64,... ){
# Importing incarceration data
td <- tempdir()  # create a temporary directory.  irt stands for incarceration rate.
irt = tempfile(tmpdir=td, fileext=".xlsx") # create the placeholder file
download.file(url=file1, destfile=irt, mode="wb") # download into the placeholder file
inc <- read.xlsx(file=irt, sheetIndex=sheetIndex1,startRow=startRow1, endRow=endRow1)  # reading in the data
# Some data Cleaining
names(inc)[names(inc)=="NA."] <- "state"
# moving the Jurisdiction and state names into one column
inc$Jurisdiction1[is.na(inc$Jurisdiction)] <-  as.character(inc$state[is.na(inc$Jurisdiction)])
inc$Jurisdiction1[!is.na(inc$Jurisdiction)] <-  as.character(inc$Jurisdiction[!is.na(inc$Jurisdiction)])
# Adding a state or US, which is the total, to the descrption description
inc$desc[is.na(inc$Jurisdiction)] <-  "state"
inc$desc[!is.na(inc$Jurisdiction)] <- "US"
# cleaing up variables and getting rid of temp variables
inc$Jurisdiction <-  inc$Jurisdiction1
inc$state <- NULL
inc$Jurisdiction1 <- NULL
#getting special characters out of Jurisdiction '|' means or
inc$Jurisdiction <- gsub(inc$Jurisdiction,pattern="/a|/b|/c|/d|/e",replacement="")
# getting rid of the -- which stands for missing
inc <- as.data.frame(sapply(inc,gsub,pattern="--",replacement=NA))
# there must be a way to do this in a loop... :)
inc$X1978<-as.numeric(inc$X1978)
inc$X1979<-as.numeric(inc$X1979)
inc$X1980<-as.numeric(inc$X1980)
inc$X1981<-as.numeric(inc$X1981)
inc$X1982<-as.numeric(inc$X1982)
inc$X1983<-as.numeric(inc$X1983)
inc$X1984<-as.numeric(inc$X1984)
inc$X1985<-as.numeric(inc$X1985)
inc$X1986<-as.numeric(inc$X1986)
inc$X1987<-as.numeric(inc$X1987)
inc$X1988<-as.numeric(inc$X1988)
inc$X1989<-as.numeric(inc$X1989)
inc$X1990<-as.numeric(inc$X1990)
inc$X1991<-as.numeric(inc$X1991)
inc$X1992<-as.numeric(inc$X1992)
inc$X1993<-as.numeric(inc$X1993)
inc$X1994<-as.numeric(inc$X1994)
inc$X1995<-as.numeric(inc$X1995)
inc$X1996<-as.numeric(inc$X1996)
inc$X1997<-as.numeric(inc$X1997)
inc$X1998<-as.numeric(inc$X1998)
inc$X1999<-as.numeric(inc$X1999)
inc$X2000<-as.numeric(inc$X2000)
inc$X2001<-as.numeric(inc$X2001)
inc$X2002<-as.numeric(inc$X2002)
inc$X2003<-as.numeric(inc$X2003)
inc$X2004<-as.numeric(inc$X2004)
inc$X2005<-as.numeric(inc$X2005)
inc$X2006<-as.numeric(inc$X2006)
inc$X2007<-as.numeric(inc$X2007)
inc$X2008<-as.numeric(inc$X2008)
inc$X2009<-as.numeric(inc$X2009)
inc$X2010<-as.numeric(inc$X2010)
inc$X2011<-as.numeric(inc$X2011)
inc$X2012<-as.numeric(inc$X2012)
inc$X2013<-as.numeric(inc$X2013)
# Re-arrange the data
inc2 <- melt(data=inc,id.vars=c("Jurisdiction","desc"),value.name="IncrRt",variable.name="year")
inc2$year <- gsub(pattern="X",replacement="",x=inc2$year)  # getting rid of the X in front of year
inc2$year <- as.numeric(inc2$year) #make year numeric
out <- inc2
return(out)
}
